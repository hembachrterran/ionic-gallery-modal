{
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "baseUrl": "./src",
    "rootDir": "./src",
    "paths": { "@angular/*": [ "../node_modules/@angular/*" ] },
    "skipLibCheck": true,
    "types": []
  }
}
